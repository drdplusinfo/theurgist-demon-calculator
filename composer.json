{
  "name": "drdplus/theurgist-demon-calculator",
  "license": "MIT",
  "type": "project",
  "description": "Configurator of demons for Drd+ theurgist",
  "autoload": {
    "psr-4": {
      "DrdPlus\\": "DrdPlus/"
    }
  },
  "repositories": [{
    "url": "/home/jaroslav/projects/drdplus/codes",
    "type": "vcs"
  }
  ],
  "require": {
    "php": ">=7.1",
    "drdplus/calculator-skeleton": "~1.0",
    "drdplus/tables": "~2.1"
  },
  "require-dev": {
    "composer/composer": "^1.8",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~7.0",
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "pre-autoload-dump": [
      "rm -f DrdPlus/Tests/CalculatorSkeleton && ln -s ../../vendor/drdplus/calculator-skeleton/DrdPlus/Tests/CalculatorSkeleton DrdPlus/Tests/"
    ]
  }
}
